{
  "name": "Playback Speed Control",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Scrub the speed of recorded videos on Blackboard Collaborate.",
  "content_scripts": [
    {
      "matches": [
        "*://*.bbcollab.com/collab/ui/session/playback*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
        "128": "icon.png"
      }
}
